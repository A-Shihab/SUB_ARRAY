#include<bits/stdc++.h>
using namespace std;

#define ll long long 

int main()
{
    ll n;
    cin>>n;
    
    vector<ll>vec(n);
    
    for(ll i=0; i<n; i++)cin>>vec[i];
    
    for(ll i=0; i<n; i++)
    {
        for(ll j=i; j<n; j++)
        {
            for(ll k=i; k<=j; k++)
            {
                cout<<vec[k]<<" ";
            }
            cout<<endl;
        }
    }
}
